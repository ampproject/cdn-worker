{
  "bracketSpacing": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "quoteProps": "preserve",
  "overrides": [
    {
      "files": ".eslintrc.json",
      "options": {"parser": "json5"}
    },
    {
      "files": "*.md",
      "options": {
        "parser": "markdown",
        "embeddedLanguageFormatting": "off",
        "tabWidth": 4
      }
    },
    {
      "files": [".prettierrc", "*.json"],
      "excludeFiles": ["package-lock.json"],
      "options": {"parser": "json"}
    },
    {
      "files": "*.js",
      "excludeFiles": ["src/schema/generated/amp_exp_schema.js"]
    }
  ]
}
